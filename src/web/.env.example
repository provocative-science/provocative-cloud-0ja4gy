# API Configuration
# Base URL for REST API endpoints (required)
# Format: http(s)://<host>:<port>
VITE_API_BASE_URL=http://localhost:8000

# WebSocket endpoint for real-time updates (required)
# Format: ws(s)://<host>:<port>
VITE_WS_BASE_URL=ws://localhost:8000

# Authentication Configuration
# Google OAuth 2.0 client ID (required)
# Format: <client-id>.apps.googleusercontent.com
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Stripe payment integration public key (required)
# Format: pk_test_<key> or pk_live_<key>
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key

# Application Configuration
# Application name displayed in UI (required)
VITE_APP_NAME=Provocative Cloud

# Application version for tracking (required)
# Format: semantic versioning (major.minor.patch)
VITE_APP_VERSION=1.0.0

# Environment name (required)
# Valid values: development, staging, production
VITE_APP_ENV=development

# Performance Configuration
# Metrics refresh interval in milliseconds (required)
# Minimum: 5000, Maximum: 60000
VITE_METRICS_REFRESH_INTERVAL=10000

# WebSocket reconnection interval in milliseconds (required)
# Minimum: 1000, Maximum: 30000
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000

# Feature Flags
# Enable GPU metrics collection and display (optional)
# Valid values: true, false
VITE_GPU_METRICS_ENABLED=true

# Maximum number of concurrent GPU rentals per user (required)
# Minimum: 1, Maximum: 10
VITE_MAX_CONCURRENT_RENTALS=5

# Localization Configuration
# Default currency for pricing display (required)
# Valid values: USD, EUR, GBP
VITE_DEFAULT_CURRENCY=USD

# Analytics Configuration
# Enable usage analytics collection (optional)
# Valid values: true, false
VITE_ENABLE_ANALYTICS=true

# Logging Configuration
# Application logging level (required)
# Valid values: debug, info, warn, error
VITE_LOG_LEVEL=info

# NOTE: This is an example configuration file.
# 1. Copy this file to .env and replace placeholder values with actual configuration
# 2. Ensure all required variables are set before starting the application
# 3. Never commit actual credentials or sensitive values to version control
# 4. Use appropriate values based on deployment environment
# 5. All variables must be prefixed with VITE_ to be exposed to the application